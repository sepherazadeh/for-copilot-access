{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Cost-Gate Orchestrator",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/orchestrator-cost-gate.js",
      "args": ["3000"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Cost-Gate Orchestrator",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/orchestrator-cost-gate.js",
      "args": ["3000"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Override Models Script",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/override-models-until.js",
      "args": ["${workspaceFolder}/agents-config-safety.json"],
      "console": "integratedTerminal"
    }
  ]
}
